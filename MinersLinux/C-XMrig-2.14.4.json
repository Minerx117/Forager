    "CnGpu": "-a cn/gpu",
    "CnHalf": "-a cn/half",
    "CnHaven": "-a cn-heavy/xhv",
    "CnHeavy": "-a cn-heavy",
    "CnLiteV7": "-a cn-lite/1",
    "CnR": "-a cn/r",
    "CnRwz": "-a cn/rwz",
    "CnSaber": "-a cn-heavy/tube",
    "CnTurtle": "-a cn-pico/trtl",
    "CnWow": "-a cn/wow",
    "CnXTL": "-a cn/xtl",
