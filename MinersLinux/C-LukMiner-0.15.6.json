{
  "Path": "luk-cpu",
  "Uri": "http://www.lukminer.net/releases/lukMiner-0.15.6-cpu-phi.tgz",
  "SHA256": "",
  "Type": "CPU",
  "Arguments": [
    "--host #Server#",
    "--port #Port#",
    "--user #Login#",
    "--pass #Password#",
    "$(if ($Pool.PoolName -eq 'NiceHash'){'--nicehash'})",
    "-t $(if ($CpuThreads -ne $null){$CpuThreads} else {$SystemInfo.Processors})",
    "--status-port #APIPort#",
    "-l luk.log",
    "#AlgorithmParameters#"
  ],
  "API": "Ccminer",
  "APIPort": 4048,
  "Algorithms": {
    "Cn": "-a xn",
    "CnAlloy": "-a xnalloy",
    "CnDark": "-a xndark",
    "CnHalf": "-a xnmasari",
    "CnHaven": "-a xnhaven",
    "CnHeavy": "-a xnheavy",
    "CnLiteV7": "-a xnlight",
    "CnR": "-a xmr-v4r",
    "CnTurtle": "-a xnlightv1",
    "CnV7": "-a xmr-v7",
    "CnV8": "-a xmr-v8",
    "CnXTL": "-a xnstellite"
  }
}