{
  "Path": "luk-cpu",
  "Uri": "https://github.com/yuzi-co/miners-linux/releases/download/2019.03/lukMiner-0.15.6-cpu-phi.tar.gz",
  "SHA256": "",
  "Type": "_CPU",
  "Arguments": [
    "--host #Server#",
    "--port #Port#",
    "--user #Login#",
    "--pass #Password#",
    "$(if ($Pool.PoolName -eq 'NiceHash'){'--nicehash'})",
    "$(if ($CpuThreads -ne $null){'-t ' + $CpuThreads})",
    "--status-port #APIPort#",
    "#AlgorithmParameters#"
  ],
  "API": "Luk",
  "APIPort": 4048,
  "Algorithms": {
    "Cn": "-a xn",
    "CnAlloy": "-a xnalloy",
    "CnDark": "-a xndark",
    "CnHalf": "-a xnmasari",
    "CnHaven": "-a xnhaven",
    "CnHeavy": "-a xnheavy",
    "_CnLiteV7": "-a xnlight",
    "CnR": "-a xmr-v4r",
    "_CnTurtle": "-a xnlightv1",
    "CnV7": "-a xmr-v7",
    "CnV8": "-a xmr-v8",
    "CnXTL": "-a xnstellite"
  }
}
