    "Equihash150": {
      "Params": "--algo 150_5",
      "Mem": 2.9
    },
    "Equihash192": {
      "Params": "--algo 192_7 --pers auto",
      "Mem": 2.75
    },
    "Equihash210": "--algo 210_9 --pers AION0PoW",
    "Ethash": {
      "Params": "--algo ethash --proto stratum",
      "Fee": 0.0065
    },
    "Ethash_Eaglesong": {
      "Params": "--algo eth+ckb --proto stratum --dserver #ServerDual#:#PortDual# --duser #LoginDual# --dpass #PasswordDual#",
      "Fee": 0.03
    },
    "Vds": "--algo vds"
  }
