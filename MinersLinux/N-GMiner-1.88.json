  "Type": "NVIDIA",
  "CUDA": 9.0,
  "Arguments": [
    "--server #Server#:#Port#",
    "--user #Login#",
    "--pass #Password#",
    "--api #APIPort#",
    "--watchdog 0",
    "--ssl $(if($EnableSSL){1}else{0})",
    "--devices #DevicesETHMode#",
    "--cuda 1",
    "--opencl 0",
    "--color 0",
    "#AlgorithmParameters#",
    "#CustomParameters#"
  ],
  "API": "GMiner",
  "SSL": true,
  "Fee": 0.02,
  "Algorithms": {
    "BeamV2": "--algo BeamHashII",
    "BFC": {
      "Params": "--algo bfc",
      "Fee": 0.03
    },
    "Cortex": {
      "Params": "--algo cortex",
      "Fee": 0.05
    },
    "Cuckoo29": {
      "Params": "--algo aeternity",
      "NoCpu": true,
      "Mem": 3.8
    },
    "Cuckaroo29": {
      "Params": "--algo grin29",
      "NoCpu": true,
      "Mem": 3.8
    },
    "Cuckaroo29d": {
      "Params": "--algo grin29",
      "NoCpu": true,
      "Mem": 3.8
    },
    "Cuckaroo29s": {
      "Params": "--algo swap",
      "NoCpu": true,
      "Mem": 3.8
    },
    "Cuckatoo31": {
      "Params": "--algo grin31",
      "NoCpu": true,
      "Mem": 7.4
    },
    "Eaglesong": "--algo eaglesong",
    "Equihash96": "--algo 96_5 --pers auto",
    "Equihash125": "--algo 125_4",
    "Equihash144": {
      "Params": "--algo 144_5 --pers auto",
