{
  "Path": "wildrig-multi",
  "Uri": "https://github.com/yuzi-co/miners-linux/releases/download/2019.03/wildrig-multi-linux-0.15.4-preview8.tar.gz",
  "SHA256": "B132B81A8AFB67794C8BC269594DF93770734DB6D4C9D5CFD0BA4ACCF80C6800",
  "Type": "AMD",
  "Arguments": [
    "-o #Server#:#Port#",
    "-u #Login#",
    "-p #Password#",
    "-r 10",
    "-R 3",
    "--opencl-platform #GpuPlatform#",
    "--opencl-devices #Devices#",
    "--send-stale",
    "--good-risers",
    "--donate-level 1",
    "--no-color",
    "--api-port #ApiPort#",
    "#AlgorithmParameters#"
  ],
  "API": "XMRig",
  "APIPort": 4028,
  "Fee": 0.01,
  "BenchmarkArg": "$(if ($AlgoName -in @('bitcore', 'x16r', 'x16rt', 'x16s', 'x21s', 'hex', 'timetravel')){'--benchmark'})",
  "Algorithms": {
    "Aergo": "--algo aergo --opencl-threads auto --opencl-launch auto",
    "Astralhash": "--algo glt-astralhash --opencl-threads auto --opencl-launch auto",
    "BCD": "--algo bcd --opencl-threads auto --opencl-launch auto",
    "Bitcore": "--algo bitcore --opencl-threads auto --opencl-launch auto",
    "BMW512": "--algo bmw512 --opencl-threads auto --opencl-launch auto",
    "C11": "--algo c11 --opencl-threads auto --opencl-launch auto",
    "Dedal": "--algo dedal --opencl-threads auto --opencl-launch auto",
    "Exosis": "--algo exosis --opencl-threads auto --opencl-launch auto",
    "Geek": "--algo geek --opencl-threads auto --opencl-launch auto",
    "Hex": "--algo hex --opencl-threads auto --opencl-launch auto",
    "HMQ1725": "--algo hmq1725 --opencl-threads auto --opencl-launch auto",
    "Jeonghash": "--algo glt-jeonghash --opencl-threads auto --opencl-launch auto",
    "Lyra2v3": "--algo lyra2v3 --opencl-threads auto --opencl-launch auto",
    "Lyra2vc0ban": "--algo lyra2vc0ban --opencl-threads auto --opencl-launch auto",
    "Padihash": "--algo glt-padihash --opencl-threads auto --opencl-launch auto",
    "Pawelhash": "--algo glt-pawelhash --opencl-threads auto --opencl-launch auto",
    "Phi": "--algo phi --opencl-threads auto --opencl-launch auto",
    "Polytimos": "--algo polytimos --opencl-threads auto --opencl-launch auto",
    "Rainforest": "--algo rainforest --opencl-threads auto --opencl-launch auto",
    "Renesis": "--algo renesis --opencl-threads auto --opencl-launch auto",
    "SHA256q": "--algo sha256q --opencl-threads auto --opencl-launch auto",
    "SHA256t": "--algo sha256t --opencl-threads auto --opencl-launch auto",
    "Skunk": "--algo skunkhash --opencl-threads auto --opencl-launch auto",
    "SonoA": "--algo sonoa --opencl-threads auto --opencl-launch auto",
    "Timetravel": "--algo timetravel --opencl-threads auto --opencl-launch auto",
    "Tribus": "--algo tribus --opencl-threads auto --opencl-launch auto",
    "X16r": "--algo x16r --opencl-threads auto --opencl-launch auto",
    "X16rt": "--algo x16rt --opencl-threads auto --opencl-launch auto",
    "X16s": "--algo x16s --opencl-threads auto --opencl-launch auto",
    "X17": "--algo x17 --opencl-threads auto --opencl-launch auto",
    "X18": "--algo x18 --opencl-threads auto --opencl-launch auto",
    "X20r": "--algo x20r --opencl-threads auto --opencl-launch auto",
    "X21s": "--algo x21s --opencl-threads auto --opencl-launch auto",
    "X22i": "--algo x22i --opencl-threads auto --opencl-launch auto"
  }
}
