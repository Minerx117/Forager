  "Type": "AMD",
  "Arguments": [
    "$(if ($enableSSL){'-O'}else{'-o'}) #Server#:#Port#",
    "-u #Login#",
    "-p #Password#",
    "-r #WorkerName#",
    "--noNVIDIA",
    "--noAMD",
    "--noUAC",
    "$(if ($Pool.PoolName -eq 'NiceHash'){'--use-nicehash'})",
    "--httpd #APIPort#",
    "--cpu #GroupName#-#Algorithm#.txt",
    "#AlgorithmParameters#",
    "#CustomParameters#"
  ],
  "API": "XMRig",
  "Fee": 0.02,
  "SSL": true,
  "Algorithms": {
    "RandomX": "--currency randomx"
  }
}
