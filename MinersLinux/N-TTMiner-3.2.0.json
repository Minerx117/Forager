    "Arguments": [
        "-o #Protocol#://#Server#:#Port#",
        "-u #Login#",
        "-p #Password#",
        "-worker #WorkerName#",
        "--nvidia",
        "-nui",
        "-luck",
        "-d #Devices#",
        "-b 0.0.0.0:#APIPort#",
        "#CustomParameters#"
    ],
    "API": "Claymore",
    "Fee": 0.01,
    "Algorithms": {
        "Ethash": {
            "Params": "-a ETHASH",
            "Mem": 3
        },
        "Kadena": "-a Blake2s -coin kda",
        "Lyra2v3": "-a LYRA2V3",
        "MTP": {
            "Params": "-a MTP",
            "Mem": 4.5
        },
        "ProgPOW": "-a PROGPOW",
        "ProgPOWZ": "-a PROGPOWZ",
        "ProgPOW092": "-a PROGPOW092",
        "ProgPOWHora": "-a PROGPOW092 -coin hora",
        "ProgPOWSero": "-a PROGPOW092 -coin sero",
        "Ubqhash": {
            "Params": "-a UBQHASH",
            "Mem": 3
        }
    }
