{
  "Path": "kbminer.exe",
  "Uri": "https://github.com/mcarloai/minerbabe-kbminer/releases/download/1.3.4/kbminer-vds-win-1.3.4.zip",
  "SHA256": "CACAAD0D4B7496CE000138C196F7D04B753D167D994578DBCC1004F44313299B",
  "Type": "AMD",
  "Arguments": [
    "--pool #Server#:#Port#",
    "--user #Login#",
    "--pass #Password#",
    "$(($DeviceGroup.DevicesArray | % {'--gpu ' + $_}) -join ' ')",
    "--enableapi",
    "--apiaddr 0.0.0.0:#ApiPort#",
    "$(if ($EnableSSL){'--enabletls'})",
    "--checkdifficulty",
    "#AlgorithmParameters#",
    "#CustomParameters#"
  ],
  "API": "KBMiner",
  "SSL": true,
  "NoCpu": true,
  "Fee": 0.02,
  "Algorithms": {
    "VDS": "--algorithm vds"
  }
}
