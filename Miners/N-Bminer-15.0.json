{
    "Path": "bminer-lite-v15.0.0-e143cb4\\bminer.exe",
    "Uri": "https://www.bminercontent.com/releases/bminer-lite-v15.0.0-e143cb4-amd64.zip",
    "SHA256": "8ACF357EF48CF912D705BEC7A7AC7C7B659B36CC33FCC011E579B5B1D0AD5DE4",
    "Type": "NVIDIA",
    "Arguments": [
        "#AlgorithmParameters#$(if($enableSSL){'+ssl'})://$([System.Web.HttpUtility]::UrlEncode('#Login#')):$([System.Web.HttpUtility]::UrlEncode('#Password#'))@#Server#:#Port#",
        "-devices #Devices#",
        "-max-network-failures=3",
        "-watchdog=false",
        "-api 127.0.0.1:#APIPort#"
    ],
    "API": "BMiner",
    "APIPort": 4068,
    "SSL": true,
    "Fee": 0.02,
    "CUDA": 9.2,
    "NoCPUMining": "$(if ($AlgoName -in @('Aeternity', 'Cuckaroo29', 'Cuckatoo31')){$true}else{$false})",
    "Algorithms": {
        "Aeternity": "-uri aeternity",
        "Cuckaroo29|6gb": "-uri cuckaroo29",
        "Cuckatoo31|9gb": "-uri cuckattoo31",
        "Equihash144": "-pers auto -uri equihash1445",
        "Equihash150|8gb": "-uri beam",
        "Tensority": "-uri tensority"
    }
}