{
  "Path": "miner.exe",
  "Uri": "https://github.com/develsoftware/GMinerRelease/releases/download/1.73/gminer_1_73_windows64.zip",
  "Type": "AMD",
  "Arguments": [
    "--server #Server#:#Port#",
    "--user #Login#",
    "--pass #Password#",
    "--api #APIPort#",
    "--watchdog 0",
    "--ssl $(if($EnableSSL){1}else{0})",
    "--devices #DevicesETHMode#",
    "--cuda 0",
    "--opencl 1",
    "#AlgorithmParameters#",
    "#CustomParameters#"
  ],
  "API": "GMiner",
  "SSL": true,
  "Fee": 0.02,
  "Algorithms": {
    "BFC": {
      "Params": "--algo bfc",
      "Fee": 0.03
    },
    "Cuckoo29": {
      "Params": "--algo aeternity",
      "NoCpu": true,
      "Mem": 5
    },
    "Cuckaroo29": {
      "Params": "--algo grin29",
      "NoCpu": true,
      "Mem": 5
    },
    "Cuckaroo29s": {
      "Params": "--algo swap",
      "NoCpu": true,
      "Mem": 5
    },
    "Eaglesong": "--algo eaglesong",
    "Equihash125": "--algo 125_4",
    "Equihash144": {
      "Params": "--algo 144_5 --pers auto",
      "Mem": 2
    },
    "Equihash150": {
      "Params": "--algo 150_5",
      "Mem": 4
    },
    "Equihash192": {
      "Params": "--algo 192_7 --pers auto",
      "Mem": 2.75
    }
  }
}
