{
  "Path": "xmrig-5.5.3/xmrig.exe",
  "Uri": "https://github.com/xmrig/xmrig/releases/download/v5.5.3/xmrig-5.5.3-msvc-cuda10_1-win64.zip",
  "Type": "NVIDIA",
  "CUDA": 10.1,
  "Arguments": [
    "-o #Server#:#Port#",
    "-u #Login#",
    "-p #Password#",
    "$(if ($Pool.PoolName -eq 'NiceHash'){'--nicehash'})",
    "$(if ($EnableSSL){'--tls'})",
    "--no-cpu",
    "--cuda",
    "--cuda-devices=#Devices#",
    "--donate-level 1",
    "--http-port #APIPort#",
    "#AlgorithmParameters#",
    "#CustomParameters#"
  ],
  "API": "XMRig",
  "SSL": true,
  "Fee": 0.01,
  "Algorithms": {
    "_Argon2d512": "-a argon2/chukwa",
    "_Argon2d256": "-a argon2/wrkz",
    "CnAlloy": "-a cn/xao",
    "CnArto": "-a cn/rto",
    "CnDouble": "-a cn/double",
    "CnFast": "-a cn/fast",
    "_CnGpu": "-a cn/gpu",
    "_CnHalf": "-a cn/half",
    "_CnHaven": "-a cn-heavy/xhv",
    "_CnHeavy": "-a cn-heavy/0",
    "CnLiteV7": "-a cn-lite/1",
    "CnR": "-a cn/r",
    "CnRwz": "-a cn/rwz",
    "_CnSaber": "-a cn-heavy/tube",
    "CnTurtle": "-a cn-pico/trtl",
    "CnTlo": "-a cn-pico/tlo",
    "CnZls": "-a cn/zls",
    "_RandomSfx": "-a rx/sfx",
    "_RandomX": "-a rx",
    "_RandomXArq": "-a rx/arq",
    "_RandomXL": "-a rx/loki",
    "_RandomXWow": "-a rx/wow"
  }
}
