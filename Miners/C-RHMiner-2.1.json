{
  "Path": "rhminer.exe",
  "Uri": "https://github.com/polyminer1/rhminer/releases/download/2.1/rhminer.2.1.Windows.CPU.zip",
  "Type": "CPU",
  "Arguments": [
    "-s #Protocol#://#Server#:#Port#",
    "-su #Login#",
    "-pw #Password#",
    "-r 5",
    "-cpu",
    "-processpriority 2",
    "-cputhreads $(if ($CpuThreads -ne $null){$CpuThreads} else {$SystemInfo.CpuFeatures.threads})",
    "-apiport #ApiPort#",
    "#CustomParameters#"
  ],
  "API": "Claymore",
  "Fee": 0.01,
  "Algorithms": {
    "RandomHash2": ""
  }
}
