{
  "Path": "cpuminer-$($f=$SystemInfo.CpuFeatures;$(if($f.avx2 -and $f.sha){'zen'}elseif($f.avx2){'avx2'}elseif($f.avx){'avx'}elseif($f.aes -and $f.sse42){'aes-sse42'}else{'sse2'})).exe",
  "Uri": "https://github.com/JayDDee/cpuminer-opt/releases/download/v3.11.8/cpuminer-opt-3.11.8-windows.zip",
  "Type": "CPU",
  "Arguments": [
    "-o #Protocol#://#Server#:#Port#",
    "-u #Login#",
    "-p #Password#",
    "--retry-pause 5",
    "-r 10",
    "-b #APIPort#",
    "$(if ($CpuThreads -ne $null){'-t ' + $CpuThreads})",
    "#AlgorithmParameters#",
    "#CustomParameters#"
  ],
  "API": "Ccminer",
  "Algorithms": {
    "CpuPower": "-a yespower -N 2048 -R 32 -K \"CPUpower: The number of CPU working or available for proof-of-work mining\"",
    "HOdl": "-a hodl",
    "Lyra2h": "-a lyra2h",
    "Lyra2z330": "-a lyra2z330",
    "Power2b": "-a yespower-b2b -N 2048 -R 32 -K \"Now I am become Death, the destroyer of worlds\"",
    "ScryptN": "-a scrypt -N 2048",
    "_SHA3d": "-a sha3d",
    "Verium": "-a scrypt -N 1048576",
    "Yescrypt": "-a yescrypt",
    "YescryptR8": "-a yescrypt -N 2048 -R 8 -K \"Client Key\"",
    "YescryptR8g": "-a yescryptr8g",
    "YescryptR16": "-a yescrypt -N 4096 -R 16 -K \"Client Key\"",
    "YescryptR32": "-a yescrypt -N 4096 -R 32 -K \"WaviBanana\"",
    "Yespower": "-a yespower -N 2048 -R 32",
    "YespowerR16": "-a yespower -N 4096 -R 16"
  }
}
