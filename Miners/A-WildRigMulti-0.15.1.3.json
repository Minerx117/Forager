{
    "Path": "wildrig.exe",
    "Uri": "https://github.com/andru-kun/wildrig-multi/releases/download/0.15.1/wildrig-multi-windows-0.15.1.3-beta.7z",
    "SHA256": "B09B179E9EE4D77464998F9251A567A50128238E463220C56E2EE88EB09D0AD4",
    "Type": "AMD",
    "Arguments": [
        "-o #Server#:#Port#",
        "-u #Login#",
        "-p #Password#",
        "-r 4",
        "-R 10",
        "--opencl-platform #GpuPlatform#",
        "--opencl-devices #Devices#",
        "--send-stale",
        "--donate-level 1",
        "--api-port #ApiPort#",
        "#AlgorithmParameters#"
    ],
    "BenchmarkArg": "$(if ($AlgoName -in @('bitcore', 'x16r', 'x16rt', 'x16s', 'hex', 'timetravel')){'--benchmark'})",
    "API": "XMRig",
    "APIPort": 4028,
    "Fee": 0.01,
    "PrelaunchCommand": "setx GPU_MAX_WORKGROUP_SIZE 256",
    "Algorithms": {
        "Aergo": "--algo aergo --opencl-threads 2 --opencl-launch 17x0",
        "BCD": "--algo bcd --opencl-threads 2 --opencl-launch 20x0",
        "Bitcore": "--algo bitcore --opencl-threads 2 --opencl-launch 19x0",
        "C11": "--algo c11 --opencl-threads 2 --opencl-launch 19x0",
        "Dedal": "--algo dedal --opencl-threads 2 --opencl-launch 20x0",
        "Exosis": "--algo exosis",
        "Geek": "--algo geek --opencl-threads 2 --opencl-launch 20x128",
        "Hex": "--algo hex --opencl-threads 2 --opencl-launch 23x0",
        "HMQ1725": "--algo hmq1725 --opencl-threads 2 --opencl-launch 20x128",
        "Jeonghash": "--algo glt-jeonghash --opencl-threads 2 --opencl-launch 17x0",
        "Lyra2v3": "--algo lyra2v3 --opencl-threads 1 --opencl-launch 23x0",
        "Lyra2vc0ban": "--algo lyra2vc0ban --opencl-threads 1 --opencl-launch 23x0",
        "MTP|8gb": "--algo mtp --opencl-threads 1 --opencl-launch 16x0",
        "Padihash": "--algo glt-padihash --opencl-threads 2 --opencl-launch 17x0",
        "Pawelhash": "--algo glt-pawelhash --opencl-threads 2 --opencl-launch 17x0",
        "Phi": "--algo phi --opencl-threads 3 --opencl-launch 19x0",
        "Polytimos": "--algo polytimos --opencl-threads 2 --opencl-launch 19x0",
        "Renesis": "--algo renesis --opencl-threads 3 --opencl-launch 18x128",
        "SHA256q": "--algo sha256q --opencl-threads 1 --opencl-launch 29x0",
        "SHA256t": "--algo sha256t --opencl-threads 1 --opencl-launch 29x0",
        "Skunk": "--algo skunkhash --opencl-threads 3 --opencl-launch 18x0",
        "SonoA": "--algo sonoa --opencl-threads 2 --opencl-launch 18x0",
        "Timetravel": "--algo timetravel --opencl-threads 2 --opencl-launch 17x128",
        "Tribus": "--algo tribus --opencl-threads 2 --opencl-launch 21x0",
        "X16r": "--algo x16r --opencl-threads 2 --opencl-launch 20x0",
        "X16rt": "--algo x16rt --opencl-threads 2 --opencl-launch 20x0",
        "X16s": "--algo x16s --opencl-threads 2 --opencl-launch 20x0",
        "X17": "--algo x17 --opencl-threads 2 --opencl-launch 20x0",
        "X18": "--algo x18 --opencl-threads 2 --opencl-launch 20x0",
        "X20r": "--algo x20r --opencl-threads 2 --opencl-launch 20x0",
        "X21s": "--algo x21s --opencl-threads 2 --opencl-launch 20x0",
        "X22i": "--algo x22i --opencl-threads 2 --opencl-launch 18x0"
    }
}