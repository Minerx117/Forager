{
    "Path": "miner.exe",
    "Uri": "https://github.com/yuzi-co/miners/releases/download/2019.02/gminer_1_30_minimal_windows64.zip",
    "SHA256": "12C9F913187526A368D1D6517D9A51A9622D9785DE18AC59FD28BF77FCD2311C",
    "Type": "NVIDIA",
    "Arguments": [
        "--server #Server#",
        "--port #Port#",
        "--user #Login#",
        "--pass #Password#",
        "--api #APIPort#",
        "$(if($enableSSL){'--ssl 1'})",
        "--devices #DevicesETHMode#",
        "#AlgorithmParameters#"
    ],
    "API": "GMiner",
    "APIPort": 4068,
    "SSL": true,
    "Fee": 0.02,
    "CUDA": 9.0,
    "NoCPUMining": "$(if ($AlgoName -in @('Cuckaroo29', 'Aeternity')){$true}else{$false})",
    "Algorithms": {
        "Aeternity": "--algo aeternity",
        "Cuckaroo29|6gb": "--algo grin29",
        "Equihash144": "--algo 144_5 --pers auto",
        "Equihash150|4gb": "--algo 150_5",
        "Equihash192|3gb": "--algo 192_7 --pers auto",
        "Equihash210": "--algo 210_9 --pers AION0PoW",
        "Equihash96": "--algo 96_5 --pers auto"
    }
}