    "#AlgorithmParameters#",
    "#CustomParameters#"
  ],
  "API": "XMRig",
  "SSL": true,
  "Fee": 0.01,
  "Algorithms": {
    "Argon2Chukwa": "-a argon2/chukwa",
    "Argon2Wrkz": "-a argon2/wrkz",
    "CnAlloy": "-a cn/xao",
    "CnArto": "-a cn/rto",
    "CnDouble": "-a cn/double",
    "CnFast": "-a cn/fast",
    "CnGpu": "-a cn/gpu",
    "CnHalf": "-a cn/half",
    "CnHaven": "-a cn-heavy/xhv",
    "CnHeavy": "-a cn-heavy",
    "CnLiteV7": "-a cn-lite/1",
    "CnR": "-a cn/r",
    "CnRwz": "-a cn/rwz",
    "CnSaber": "-a cn-heavy/tube",
    "CnTurtle": "-a cn-pico/trtl",
    "CnWow": "-a cn/wow",
    "CnZls": "-a cn/zls",
    "RxWow": "-a Rx/wow",
    "RxXL": "-a Rx/loki"
