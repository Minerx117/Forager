{
    "Path": "miner.exe",
    "Uri": "https://github.com/yuzi-co/miners/releases/download/2019.02/gminer_1_31_minimal_windows64.zip",
    "SHA256": "9DD71D88F582702A14BFCE89C85812DDE7A33555103BC0055BEA7A85CEEC9E4E",
    "Type": "NVIDIA",
    "Arguments": [
        "--server #Server#",
        "--port #Port#",
        "--user #Login#",
        "--pass #Password#",
        "--api #APIPort#",
        "$(if($enableSSL){'--ssl 1'})",
        "--devices #DevicesETHMode#",
        "#AlgorithmParameters#"
    ],
    "API": "GMiner",
    "APIPort": 4068,
    "SSL": true,
    "Fee": 0.02,
    "CUDA": 9.0,
    "NoCPUMining": "$(if ($AlgoName -in @('Aeternity', 'Cuckaroo29', 'Cuckatoo31')){$true}else{$false})",
    "Algorithms": {
        "Aeternity": "--algo aeternity",
        "Cuckaroo29|6gb": "--algo grin29",
        "Cuckatoo31|9gb": "--algo grin31",
        "Equihash144": "--algo 144_5 --pers auto",
        "Equihash150|4gb": "--algo 150_5",
        "Equihash192|3gb": "--algo 192_7 --pers auto",
        "Equihash210": "--algo 210_9 --pers AION0PoW",
        "Equihash96": "--algo 96_5 --pers auto"
    }
}