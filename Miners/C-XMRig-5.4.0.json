{
  "Path": "xmrig-5.4.0/xmrig.exe",
  "Uri": "https://github.com/xmrig/xmrig/releases/download/v5.4.0/xmrig-5.4.0-gcc-win64.zip",
  "Type": "CPU",
  "Arguments": [
    "-o #Server#:#Port#",
    "-u #Login#",
    "-p #Password#",
    "$(if ($Pool.PoolName -eq 'NiceHash'){'--nicehash'})",
    "$(if ($EnableSSL){'--tls'})",
    "--donate-level 1",
    "--http-port #APIPort#",
    "#AlgorithmParameters#",
    "#CustomParameters#"
  ],
  "API": "XMRig",
  "SSL": true,
  "Fee": 0.01,
  "Algorithms": {
    "Argon2d512": "-a argon2/chukwa",
    "Argon2d256": "-a argon2/wrkz",
    "CnAlloy": "-a cn/xao",
    "CnArto": "-a cn/rto",
    "CnDouble": "-a cn/double",
    "CnFast": "-a cn/fast",
    "CnGpu": "-a cn/gpu",
    "CnHalf": "-a cn/half",
    "CnHaven": "-a cn-heavy/xhv",
    "CnHeavy": "-a cn-heavy/0",
    "CnLiteV7": "-a cn-lite/1",
    "CnR": "-a cn/r",
    "CnRwz": "-a cn/rwz",
    "CnSaber": "-a cn-heavy/tube",
    "CnTurtle": "-a cn-pico/trtl",
    "CnZls": "-a cn/zls",
    "RandomSfx": "-a rx/sfx",
    "RandomV": "-a rx/v",
    "RandomX": "-a rx/0",
    "RandomXArq": "-a rx/arq",
    "RandomXL": "-a rx/loki",
    "RandomXWow": "-a rx/wow"
  }
}
