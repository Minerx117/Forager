    "Algorithms": {
        "Aeternity": "-uri aeternity",
        "Cuckaroo29|6gb": "-uri cuckaroo29",
        "Cuckatoo31|11gb": "-uri cuckattoo31",
        "Equihash144": "-pers auto -uri equihash1445",
        "Equihash150|8gb": "-uri beam",
        "Tensority": "-uri tensority"
    }
}