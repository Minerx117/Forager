{
    "Path": "bminer-lite-v14.3.0-cbb8683\\bminer.exe",
    "Uri": "https://www.bminercontent.com/releases/bminer-lite-v14.3.0-cbb8683-amd64.zip",
    "SHA256": "E0B9B3BADB20344B629D8D8F0EAB81143DDC8579E392C5458C425696C08A8D3C",
    "Type": "NVIDIA",
    "Arguments": [
        "#AlgorithmParameters#$(if($enableSSL){'+ssl'})://$([System.Web.HttpUtility]::UrlEncode('#Login#')):$([System.Web.HttpUtility]::UrlEncode('#Password#'))@#Server#:#Port#",
        "-devices #Devices#",
        "-max-network-failures=3",
        "-watchdog=false",
        "-api 127.0.0.1:#APIPort#"
