        "Tensority_Ethash": {
            "Params": "-a tensority_ethash -do $(if ($PoolDual.PoolName -eq 'NiceHash') {'ethnh'} else {'ethproxy'})+tcp://#ServerDual#:#PortDual# -du #LoginDual#",
            "Mem": 3,
            "Fee": 0.03
        },
        "Tensority": {
            "Params": "-a tensority",
            "Fee": 0.02
        }
    }
