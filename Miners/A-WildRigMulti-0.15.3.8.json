{
  "Path": "wildrig.exe",
  "Uri": "https://github.com/yuzi-co/miners/releases/download/2019.02/wildrig-multi-windows-0.15.3.8-beta.7z",
  "SHA256": "7F028AD51B2B9DD2FE11A4DB39A148FDEAAAFB4BA4483FAF96E30D754CF97FCE",
  "Type": "AMD",
  "Arguments": [
    "-o #Server#:#Port#",
    "-u #Login#",
    "-p #Password#",
    "-r 10",
    "-R 3",
    "--opencl-platform #GpuPlatform#",
    "--opencl-devices #Devices#",
    "--send-stale",
    "--donate-level 1",
    "--api-port #ApiPort#",
    "#AlgorithmParameters#",
    "CustomParameters"
  ],
  "BenchmarkArg": "$(if ($AlgoName -in @('bitcore', 'x16r', 'x16rt', 'x16s', 'x21s', 'hex', 'timetravel')){'--benchmark'})",
  "API": "XMRig",
  "APIPort": 4028,
  "Fee": 0.01,
  "Mem": 1.1,
  "PrelaunchCommand": "if ($env:GPU_MAX_WORKGROUP_SIZE -ne 256) { $env:GPU_MAX_WORKGROUP_SIZE = 256 }",
  "Custom": "--opencl-threads auto --opencl-launch auto",
  "Algorithms": {
    "Aergo": "--algo aergo",
    "Astralhash": "--algo glt-astralhash",
    "BCD": "--algo bcd",
    "Bitcore": "--algo bitcore",
    "BMW512": "--algo bmw512",
    "C11": "--algo c11",
    "Dedal": "--algo dedal",
    "Exosis": "--algo exosis",
    "Geek": "--algo geek",
    "Hex": "--algo hex",
    "HMQ1725": "--algo hmq1725",
    "Jeonghash": "--algo glt-jeonghash",
    "_Lyra2v3": "--algo lyra2v3",
    "Lyra2vc0ban": "--algo lyra2vc0ban",
    "Padihash": "--algo glt-padihash",
    "Pawelhash": "--algo glt-pawelhash",
    "Phi": "--algo phi",
    "Polytimos": "--algo polytimos",
    "Renesis": "--algo renesis",
    "SHA256q": "--algo sha256q",
    "SHA256t": "--algo sha256t",
    "Skunk": "--algo skunkhash",
    "SonoA": "--algo sonoa",
    "Timetravel": "--algo timetravel",
    "Tribus": "--algo tribus",
    "X16r": "--algo x16r",
    "X16rt": "--algo x16rt",
    "X16s": "--algo x16s",
    "X17": "--algo x17",
    "X18": "--algo x18",
    "X20r": "--algo x20r",
    "X21s": "--algo x21s",
    "X22i": "--algo x22i"
  }
